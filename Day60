#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, k;
    scanf("%d", &n);

    int arr[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    scanf("%d", &k);

    if (k <= 0 || k > n) {
        printf("Invalid k");
        return 0;
    }

    // Deque will store indices of useful elements
    int dq[n];
    int front = 0, rear = -1;

    // First window
    for (int i = 0; i < k; i++) {
        while (front <= rear && arr[dq[rear]] <= arr[i]) {
            rear--;
        }
        dq[++rear] = i;
    }

    // Print max of first window
    printf("%d ", arr[dq[front]]);

    // Process rest windows
    for (int i = k; i < n; i++) {

        // Remove indices outside this window
        while (front <= rear && dq[front] <= i - k) {
            front++;
        }

        // Remove smaller elements from the back
        while (front <= rear && arr[dq[rear]] <= arr[i]) {
            rear--;
        }

        dq[++rear] = i;

        printf("%d ", arr[dq[front]]);
    }

    return 0;
}

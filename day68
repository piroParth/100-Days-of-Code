#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];
    int sumArr = 0;

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        sumArr += arr[i];
    }

    // Sum of numbers from 0 to n
    int totalSum = n * (n + 1) / 2;

    int missing = totalSum - sumArr;

    printf("%d", missing);

    return 0;
}

#include <stdio.h>

#define MAX 100000  // Adjust if needed for larger inputs

int main() {
    int n, target;
    scanf("%d", &n);

    int nums[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    scanf("%d", &target);

    // Hash map to store value â†’ index (initialize with -1)
    int hash[MAX];
    for (int i = 0; i < MAX; i++)
        hash[i] = -1;

    for (int i = 0; i < n; i++) {
        int needed = target - nums[i];

        if (needed >= 0 && needed < MAX && hash[needed] != -1) {
            printf("%d %d", hash[needed], i);
            return 0;
        }

        hash[nums[i]] = i;
    }

    printf("-1 -1");
    return 0;
}

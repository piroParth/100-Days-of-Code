#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);

    int arr[n];

    // Read input and simultaneously detect repetition
    int seen[100000] = {0};   // adjust size if needed
    int repeated = -1;

    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);

        if (seen[arr[i]] == 1) {
            repeated = arr[i];
        }
        seen[arr[i]] = 1;
    }

    printf("%d", repeated);

    return 0;
}
